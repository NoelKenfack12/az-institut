	<link href="{{ asset('template/css/animate.min.css') }}" rel="stylesheet"/> 
	<script src="{{ asset('template/js/wow.min.js') }}"></script>
	<script>
	 new WOW().init();
	</script>
	
	<!-- core CSS -->
    <link href="{{ asset('template/css/bootstrap.min.css') }}" rel="stylesheet">
