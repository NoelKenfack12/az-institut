function openCardPanel()
{
	if(animtail == 0)
	{
	  $('.content-alert-action-hosting-user').show();
	  $('.content-alert-action-hosting-user').html('<div style="position: fixed; top: 0px; margin-bottom: '+$(window).scrollTop()+'px; left: 0px; z-index: 9000; width: '+($(window).width() + 100)+'px; height: '+($(window).height() + $(window).scrollTop())+'px; background: rgba(0,0,0,0.5);"></div>');
	  
	  $( ".side-nav-dashboad" ).animate({ "bottom": "+="+panelheight+"px" }, "slow" );
	  $( ".hide-nav-premuim" ).show();
	  $( ".show-nav-premuim" ).hide();
	  animtail = panelheight;
	}
}

function closeCardPanel()
{
	if(animtail == panelheight)
	{
	  $( ".side-nav-dashboad" ).animate({ "bottom": "-="+panelheight+"px"}, "slow");
	  $( ".hide-nav-premuim" ).hide();
	  $( ".show-nav-premuim" ).show();
	  $('.content-alert-action-hosting-user').hide();
	  animtail = 0;
	}
}

