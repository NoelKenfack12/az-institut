		<script type="text/javascript">
		$(function(){
			{% if nbproduit > 0 %}
			var clignotement = function(){ 
			   if (document.getElementById('DivClignotante').style.visibility=='visible'){ 
				  document.getElementById('DivClignotante').style.visibility='hidden'; 
			   } 
			   else{ 
			   document.getElementById('DivClignotante').style.visibility='visible'; 
			   } 
			}; 
			$('#DivClignotante').html({{ nbproduit }});
			// mise en place de l appel de la fonction toutes les 0.8 secondes 
			// Pour arrêter le clignotement : clearInterval(periode); 
			periode = setInterval(clignotement, 500); 
			{% endif %}
		});

		var pushPanel = (function () {
							
		var $pushPanelHeader = $(".push-panel-header"),
		   $pushPanel = $(".push-panel");

		function ShowPushPanel(delay) {
			var _delay = 300;
			if (delay) {
				_delay = delay;
			}

			setTimeout(function () {
				$pushPanel.animate({ bottom: "0" }, 800, 'easeOutBounce');
			}, 8000);
		}

		function TriggerPushPanel(delay) {   
			// Wire up click event to the push panel
			$(".push-panel-open").on('click', OpenClose);
			// get the trigger element
			var trigger = $('.push-panel-trigger');
			// check to see if there is a trigger element and if there is, is it visible
			// if there is no trigger element, we'll show the panel
			if (trigger.length === 0 || IsShown(trigger)) {
				// yes?  Show the panel
				ShowPushPanel(delay);
			}
			else {
				//  no? keep checking as the user scrolls, then show the panel when it's visible
				$(window).scroll(function () {
					if (IsShown(trigger)) {
						ShowPushPanel(300);
					}
				});
			}
		}


		function OpenClose() {
			var body = $('.push-panel .body'),
				toggleState = body.css('display'),
				headline = $(".push-panel").data("headline");

			  body.slideToggle(800, 'easeOutBounce');

			if (toggleState == 'none') {
				$pushPanelHeader.attr('class', 'push-panel-header push-panel-close');
				$(".visibility").attr('class', 'visibility fa fa-arrow-down');
			}
			else {
				$pushPanelHeader.attr('class', 'push-panel-header push-panel-open');
				$(".visibility").attr('class', 'visibility fa fa-arrow-up');
			}

		}



		// Public methods
		return {
			TriggerPushPanel: TriggerPushPanel
		};

		})();



		$(function(){
		pushPanel.TriggerPushPanel(8000);

		var tail = ($(window).height()) - 50;
		$('.content-message-push').css('height',tail+'px');

		});
		</script>

